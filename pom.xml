<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
	
	<groupId>com.codingstrain</groupId>
    <artifactId>parent-module</artifactId>
    <version>1.0-SNAPSHOT</version>
    <name>parent-module</name>
    <packaging>pom</packaging>
    
    <properties>
		<java.version>1.8</java.version>
    </properties>

    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.7.5</version>
    </parent>
       
    <modules>
        <module>java-miscellaneous-tips</module>
        <module>spring-boot-minimal-rest-h2</module>
        <module>spring-boot-minimal-rest-h2-actuator</module>
		<module>spring-cloud-config-native-server</module>
		<module>spring-cloud-config-native-client</module>
        <module>spring-cloud-config-git-server</module>
        <module>spring-cloud-config-git-client</module>
        <module>spring-cloud-discovery</module>  
        <module>spring-cloud-sample-libraryapp</module>        
    </modules>

	<build>
		<plugins>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
			</plugin>
		</plugins>
	</build>

</project>
